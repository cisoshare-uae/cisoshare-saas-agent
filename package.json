{
  "name": "agent",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node --project tsconfig.tsnode.json --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.tsnode.json",
    "start": "ts-node --project tsconfig.tsnode.json --transpile-only dist/server.js",
    "migrate": "ts-node --project tsconfig.tsnode.json --transpile-only src/migrate.ts",
    "db:reset": "npm run migrate"
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node-fetch": "^2.6.9",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.15.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
